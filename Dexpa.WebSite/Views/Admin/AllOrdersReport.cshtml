@model Dexpa.WebSite.Models.Driver.DriverViewModel

@{
    ViewBag.Title = "Отчет по всем заказам";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="loader" id="loader">
    <img src="/Content/Images/loader.gif" alt="">
</div>

<div id="main_part">
    <div class="row">
        <div class="col-sm-12" ng-controller="ReportsCtrl" ng-init="init('allOrders')">
            @Html.Partial("ReportsPartials/_AllOrdersReportPanel")
            @Html.Partial("ReportsPartials/_AllOrdersReportTable")
        </div>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/Scripts/AngularControllers/ReportsController.js")
}