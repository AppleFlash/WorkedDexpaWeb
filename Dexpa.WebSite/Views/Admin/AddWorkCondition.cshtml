@{
    ViewBag.Title = "Добавить условия работы";
}

<div class="loader" id="loader">
    <img src="/Content/Images/loader.gif" alt="">
</div>

<div id="main_part">
    <div class="row">
        <div class="container">
            <div class="row pageheader"><h2>Добавить условия работы</h2></div>
            <div class="row" ng-controller="AdminCtrl" ng-init="initNewWC()">
                <div class="containerWithoutPageHeader">
                    @Html.Partial("WorkConditionsPartials/_AddWorkConditionPartial")
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/Scripts/AngularControllers/AdminAppController.js")
}